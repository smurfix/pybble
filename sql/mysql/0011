Do("""ALTER TABLE bindata
    DROP KEY `x_8`,
    MODIFY COLUMN `id` int(11) NOT NULL DEFAULT '0',
    MODIFY COLUMN `mime_id` int(11) NULL DEFAULT NULL,
    MODIFY COLUMN `hash` tinyblob NULL DEFAULT NULL,
    MODIFY COLUMN `timestamp` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    ADD KEY `bindata_mimeid`(`mime_id`),
    ADD UNIQUE KEY `hash`(`hash`(255))""")
# <U x_8 (<bindata.hash>, None)> 
#id: integer, !len, default ''
#mime_id: integer, !len
#hash: varchar, len=30, !null, default ''
#timestamp: default NULL, !updefault

